/* pages/test/test.wxss */
.header {
  height: 500rpx;
  width: 100%;
  background-color: #fff;
  position: relative;
}

.swiper-box {
  position: absolute;
  z-index: 2;
  width: 100%;
  height: 100%;
  display: block;
  background-color: red;
}

.swiper-box swiper-item {
  display: block;
}

.swiper-box image {
  display: block;
  width: 100%;
  height: 100%;
}

.search {
  margin: 0 auto;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  position: absolute;
  z-index: 3;
  width: 80rpx;
  height: 80rpx;
  background-color: #1195db;
}